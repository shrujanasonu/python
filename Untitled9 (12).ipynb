      "source": [
        "n = int(input(\"Enter the number of rows: \"))\n",
        "for i in range(1, n + 1):\n",
        "    print(\" \" * (n - i), end=\"\")  # Print spaces for alignment\n",
        "    for j in range(1, i + 1):\n",
        "        print(j, end=\" \")\n",
        "    print()\n",
        "\n"
