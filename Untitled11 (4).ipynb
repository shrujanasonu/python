      "source": [
        "def count_vowels(string):\n",
        "    vowels = \"aeiouAEIOU\"\n",
        "    count = sum(1 for char in string if char in vowels)\n",
        "    return count\n",
        "text = input(\"Enter a string: \")\n",
        "vowel_count = count_vowels(text)\n",
        "print(f\"Number of vowels in the string: {vowel_count}\")\n"
